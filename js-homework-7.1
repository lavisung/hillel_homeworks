 function isEmailValid(emailArray) {
    const validPattern = /^[^\s@]+(\.[^\s@]+)?@(gmail\.com|yahoo\.com)$/
    const validEmails = emailArray.filter(user => validPattern.test(user.email))
    return validEmails.map(user => user.email);
}

